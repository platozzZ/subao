.cu-bar{
  color: rgb(136,136,136);
  font-size: 26rpx;
}
.cu-list.menu>.cu-item::after {
  width: calc(100% - 30rpx);
  height: 100%;
  transform: scale(1);
  left: 30rpx;
  border-bottom: 0.5rpx solid rgba(221, 221, 221,.5);
}
radio-group{
  width: 100%;
}
radio .wx-radio-input::before{
  display: block;
  content: ''
}
radio .wx-radio-input{
  width: 38rpx;
  height: 38rpx;
}
radio .wx-radio-input.wx-radio-input-checked{
  border-color:  #0060e4
}
radio .wx-radio-input.wx-radio-input-checked::before{
  font-size: 32rpx;
}
radio[checked]::before {
  content: ""!important;
}
radio::before{
  transition: none
}
