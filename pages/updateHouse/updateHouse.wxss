page{
  width: 100%;
  height: 100%;
}
.houseContainer{
  width: 100%;
  height: 100%;
}
.houseImg{
  width: 100%;
  height: auto;
}
.houseTitle{
  color: #888888;
}
.houseContent{
  flex: 1;
  position: relative;
}
.houseScroll{
  width: 100%;
  height: 100%;
}
.houseBottom{
  height: 150rpx;
}
.cu-list.menu>.cu-item::after {
  width: calc(100% - 30rpx);
  height: 100%;
  transform: scale(1);
  left: 30rpx;
  border-bottom: 1rpx solid rgba(221, 221, 221,.5);

}
.cu-list.menu>.cu-item.arrow{
  padding-right: 70rpx;
}

.houseEmpty{
  position: absolute;
  top: 50%;
  left: 50%;
  text-align: center;
  font-size: 24rpx;
  color: #999999;
  -webkit-transform: translate3d(-50%,-50%,0) scale(1);
  transform: translate3d(-50%,-50%,0) scale(1);
  z-index: 1;
}
.houseEmpty image{
  width: 166rpx;
  height: 142rpx;
  margin-bottom: 50rpx;
}
.white-nowrap{
  white-space: nowrap
}
.sourceIcon{
  width: 30rpx;
  height: 30rpx;
}