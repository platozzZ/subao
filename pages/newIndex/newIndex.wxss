.cu-list.menu .new-tag .cu-tag{
  background: rgba(255,255,255,0);
  height: 40rpx;
  line-height: 40rpx;
  padding: 0 10rpx;
  border-radius: 20rpx;
}
.cu-list.menu .new-tag .cu-tag::after{
  content: " ";
  display: block;
  width: 200%;
  height: 200%;
  position: absolute;
  top: 0;
  left: 0;
  border: 1rpx solid currentColor;
  border-radius: 40rpx;
  transform: scale(0.5);
  transform-origin: 0 0;
  box-sizing: border-box;
  z-index: 1;
  pointer-events: none;
}
.new-free.cu-tag{
  font-size: 18rpx;
  padding: 0 8rpx;
  border-radius: 22rpx;
  line-height: 27rpx;
  height: 27rpx;
}
