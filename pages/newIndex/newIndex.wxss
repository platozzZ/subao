}
.cu-list.menu>.cu-item{
  padding: 30rpx;
}
.cu-list.menu>.cu-item .action{
  position: relative;
}
.cu-btn.line-transparent{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.cu-btn.line-transparent::after{
  border: none;
}
